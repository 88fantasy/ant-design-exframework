{
  "name": "ant-design-exframework",
  "version": "1.1.3",
  "description": "exframework for front ( base on antd )",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "rm -rf es/ && rm -rf lib/ && antd-tools run compile",
    "tsc": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./components && npm run lint:style",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "exframework",
    "front",
    "antd"
  ],
  "author": "rwe",
  "license": "ISC",
  "files": [
    "dist",
    "lib"
  ],
  "devDependencies": {
    "@ant-design/pro-form": "^1.15.2",
    "@ant-design/pro-layout": "^6.13.0",
    "@ant-design/pro-table": "^2.27.2",
    "@ant-design/tools": "^13.3.3",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@stavalfi/babel-plugin-module-resolver-loader": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.11.2",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "classnames": "^2.2.6",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "css-split-webpack-plugin": "^0.2.6",
    "enzyme": "^3.11.0",
    "esbuild-webpack-plugin": "^1.1.0",
    "eslint": "^7.17.0",
    "eslint-formatter-pretty": "^4.0.0",
    "ignore-emit-webpack-plugin": "^2.0.6",
    "jest": "^26.6.3",
    "less": "^4.1.0",
    "less-loader": "^7.2.1",
    "mini-css-extract-plugin": "^1.3.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.3",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "string-replace-loader": "^3.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2",
    "umi-request": "^1.3.5",
    "url-loader": "^4.1.1"
  },
  "dependencies": {}
}
