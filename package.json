{
  "name": "ant-design-exframework",
  "version": "1.0.2",
  "description": "exframework for front ( base on antd )",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/88fantasy/exframework-front.git"
  },
  "keywords": [
    "exframework",
    "front",
    "antd"
  ],
  "author": "rwe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/88fantasy/exframework-front/issues"
  },
  "homepage": "https://github.com/88fantasy/exframework-front#readme",
  "files": [
    "dist",
    "lib"
  ],
  "devDependencies": {
    "@ant-design/pro-layout": "^6.7.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.9.4",
    "enzyme": "^3.11.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2"
  },
  "typings": "lib/index.d.ts"
}
